{
  "name": "presentations",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/presentations",
  "bugs": "https://github.com/travi/presentations/issues",
  "homepage": "htpps://presentations.travi.org",
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "test:smoke": "mocha --timeout 5000 --recursive test",
    "test": "run-s lint:* test:* build",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "start": "babel-node webpack-dev-server.js",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/travi/presentations.git\" -a -d lib/",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.5",
    "@travi/eslint-config-travi": "1.6.13",
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.19",
    "component-emitter": "1.2.1",
    "component-type": "1.2.1",
    "css-loader": "0.28.11",
    "cz-conventional-changelog": "2.1.0",
    "ease-component": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "file-loader": "1.1.11",
    "gh-pages": "1.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.14.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0-rc.2",
    "img-loader": "3.0.0",
    "jsdom": "11.10.0",
    "lodash-webpack-plugin": "0.11.5",
    "markdownlint-cli": "0.8.1",
    "mocha": "5.1.1",
    "mq-polyfill": "1.1.8",
    "mustache-loader": "1.4.0",
    "npm-run-all": "4.1.3",
    "quibble": "0.5.5",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.3.2",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.11.0",
    "webpack-config-utils": "2.3.0",
    "webpack-dev-server": "2.11.2",
    "webpack-hot-middleware": "2.22.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "normalize.css": "8.0.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-emojione": "5.0.0",
    "react-typist": "2.0.4",
    "spectacle": "3.0.0",
