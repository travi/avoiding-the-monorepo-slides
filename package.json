{
  "name": "presentations",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/presentations",
  "bugs": "https://github.com/travi/presentations/issues",
  "homepage": "htpps://presentations.travi.org",
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "test:smoke": "mocha --timeout 5000 --recursive test",
    "test": "run-s lint:* test:* build",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "start": "babel-node webpack-dev-server.js",
    "predeploy": "run-s build",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./lib --prod",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.8.2",
    "@travi/eslint-config-travi": "^1.6.18",
    "assets-webpack-plugin": "4.0.0",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.16",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "0.1.19",
    "commitlint-config-travi": "1.2.31",
    "component-emitter": "1.2.1",
    "component-type": "1.2.1",
    "css-loader": "0.28.11",
    "cz-conventional-changelog": "2.1.0",
    "ease-component": "1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "file-loader": "2.0.0",
    "gh-pages": "2.0.1",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.15.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.3",
    "img-loader": "3.0.1",
    "jsdom": "16.5.0",
    "lodash-webpack-plugin": "0.11.5",
    "markdownlint-cli": "0.13.0",
    "mocha": "10.2.0",
    "mq-polyfill": "1.1.8",
    "mustache-loader": "1.4.1",
    "netlify-cli": "^2.2.0",
    "npm-run-all": "4.1.3",
    "quibble": "0.5.5",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.4.1",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "3.11.0",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "^2.11.3",
    "webpack-hot-middleware": "2.24.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "normalize.css": "8.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-emojione": "5.0.0",
    "react-typist": "2.0.4",
    "spectacle": "3.0.0",
    "spectacle-code-slide": "0.5.2",
